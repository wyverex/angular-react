{
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "comma-dangle": 0,
        "indent": [1, 4],
        "max-len": [1, 120],
        "semi": "off",
        "space-before-function-paren": "off",
        "strict": "off"
    }
}